<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable
            name="viewmodel"
            type="com.example.meetup.viewmodel.NewsViewModel" />
    </data>
<FrameLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".view.home.EventsFragment">

   <Button
       android:onClick="@{viewmodel::clearList}"
       android:layout_gravity="center"
       android:text="UPDATE LIST"
       android:layout_width="wrap_content"
       android:layout_height="50dp"/>
    <Button
        android:onClick="@{viewmodel::fillList}"
        android:text="FILL LIST"
        android:layout_gravity="center_horizontal"
        android:layout_width="wrap_content"
        android:layout_height="50dp"/>

</FrameLayout>
</layout>